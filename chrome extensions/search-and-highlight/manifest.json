{
    "manifest_version": 3,
    "name": "Search, Find & Highlight",
    "version": "1.0",
    "description": "This extension helps find and highlight text on web page",
    "author": "S.Qadir",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "js": ["content.js"],
        "matches": [
          "http://localhost:3003/*",
          "<all_urls>"
        ]
      }
    ],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "index.html"
    }
}

